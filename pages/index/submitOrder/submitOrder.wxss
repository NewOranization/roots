page{
    background: #eee;
    font-size: 30rpx;
}

.header{
  height: 90rpx;
  line-height: 90rpx;
  background: #fe2d4a;
  color:#fff;
  text-align: center;
  position: relative;
}

.header image{
  width:40rpx;
  height:40rpx;
  position: absolute;
  top: 25rpx;
  left: 20rpx;
}

.receiveInfo{
    width: 100%;
    background: #fff;
}

.receiveInfo .info{
    width: 100%;
    border-bottom: 1px solid #eee;
}

.receiveInfo .info .defaultAddress{
    width: 100%;
}

.receiveInfo .info .defaultAddress view{
    overflow: hidden;
    line-height: 60rpx;
}

.receiveInfo .info .defaultAddress view text{
    display: block;
}

.receiveInfo .info .defaultAddress view text:first-child{
    float: left;
    margin-left: 30rpx;
}

.receiveInfo .info .defaultAddress view text:last-child{
    float: right;
    text-align: right;
    margin-right: 30rpx;
}

.receiveInfo .info .defaultAddress .address{
    margin: 0 30rpx;
    font-size: 34rpx;
    font-weight: bold;
}

.receiveInfo .info .choose-address{
    line-height: 80rpx;
    overflow: hidden;
    margin: 0 30rpx;
}

