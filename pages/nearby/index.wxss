@import "../template/tel_tab/tel_tab.wxss";
@import "../template/title/index.wxss";

.waitLoading{
    position: fixed;
    width: 100%;
    height: 1000px;
    background: #fff;
    z-index: 2000;
}

.waitLoading image{
    display: block;
    width: 200rpx;
    height: 200rpx;
    margin: 300rpx auto;
}

.box {
  margin:0;
}


.load-notice{
    width: 100%;
    text-align: center;
    font-size: 30rpx;
}

.load-notice .loading{
    width: 100%;
    text-align: center;
    height: 50rpx;
    line-height: 50rpx;
}


.load-notice .endding{
    width: 100%;
    height: 100rpx;
    line-height: 100rpx;
    background: #ededed;
    color: #8b8b8d;
    position: relative;
}

.load-notice .endding .line{
    width: 240rpx;
    height: 1px;
    background: #d8dcda;
    display: inline-block;
    position: absolute;
    left: 20rpx;
    top: 49rpx;
}
.load-notice .endding .line_r{
    width: 240rpx;
    height: 1px;
    background: #d8dcda;
    display: inline-block;
    position: absolute;
    right: 20rpx;
    top: 49rpx;
}

.formEmpty{
    width: 100%;
    text-align: center;
}

.formEmpty image{
  width: 200rpx;
  height: 210rpx;
  display: block;
  margin: 300rpx auto 20rpx;
}
